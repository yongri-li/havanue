  {
    "name": "Cross-sell Feature",
    "settings": [
      {
        "type": "paragraph",
        "content": "The Cross-sell that identifies recommended products is based on data about products that were purchased together and products with similar descriptions. It uses this data to provide a mix of those related products as recommendations for a given product."
      },
      {
        "type": "checkbox",
        "id": "crosssell_enable",
        "label": "Enable",
        "default": true
      },
      {
        "type": "range",
        "id": "crosssell_limit",
        "label": "Limit",
        "min": 1,
        "max": 30,
        "step": 1,
        "default": 10
      }
    ]
  },
  {
    "name": "Upsell Feature",
    "settings": [
      {
        "type": "checkbox",
        "id": "upsell_show_popup",
        "label": "Show popup after add product to cart",
        "default": true,
        "info": "The products exist in cart will not be displayed in this popup. Fly cart effect will be disabled if this popup is turned on."
      },
      {
        "type": "checkbox",
        "id": "upsell_show_product_page",
        "label": "Show upsell products on product page",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "upsell_show_cart_page",
        "label": "Show upsell products on cart page",
        "default": true,
        "info": "The upsell products on cart page are the mix of upsell products of each item in cart"
      }
    ]
  },
