.product-price__sale, 
.product__price--sale {
  color: $color_hover;
}
.collection-items {
  @include transition($transition-base);
  .products {
    @include display-flexbox();
    @include justify-content(left);
    width: 100%;
  }
  .product-item-wrapper {
    @media(max-width: 767px) {
      width: 100%;
    }
  }
  .row-container {
    padding-left: 0;
    padding-right: 0;
    @include border_radius(0);
    @media(min-width: 1025px) {
      &:hover {
        overflow: inherit;
        .row-right {
          .grid-mode-description {
            @include transform(scale(1,1));
            height: 100%;
            @include opacity(1,100);
          }
        }
      }
    }
  }
  &.full_width {
    .review-grid-mode {
      display: none;
    }
    .product-item-wrapper {
      width: 100%;
      @include display-flexbox();
      @include align-items(flex-start);
      background: #fff;
      .product-item-top {
        width: 25%;
        @media(max-width: 767px) {
          width: 45%;
        }
      }
      .product-item-bottom {
        width: 55% !important;
        text-align: left !important;
        padding-left: 30px !important;
        @include transition($transition-base);
        @media(max-width: 767px) {
          padding-left: 10px !important;
        }
      }
    }
    .product-list {
      .product-item {
        .product-item-wrapper {
          .product-item-top {
            .product-qs-wrapper {
              display: none;
            }
          }
          .product-item-bottom {
            @include display-flexbox();
            width: 75% !important;
            @media(max-width: 767px) {
              width: 55% !important;
            }
            .product-details {
              text-align: left;
              width: 65%;
              .spr-badge {
                display: none;
              }
              @media(max-width: 767px) {
                width: 100%;
              }
              .product-price__price {
                display: none;
              }
            }
            .actions {
              display: none;
              &.actions-list-mode {
                display: block;
                text-align: left;
                width: 35%;
                position: static;
                margin-top: 0;
                padding-left: $row_padding;
                padding-right: $row_padding;
                @media(max-width: 767px) {
                  width: 100%;
                  padding-left: 0;
                  padding-right: 0;
                }
                .qs-button {
                  margin-bottom: $row_padding;
                  @media(max-width: 767px) {
                    display: none;
                  }
                }
              }
            }
            .list-mode-description {
              @media(max-width: 767px) {
                display: none;
              }
              &.full-desc {
                display: block;
                @media(max-width: 767px) {
                  display: none;
                }
              }
            }
          }
        }
        @media(min-width: 1025px) {
          &:hover {
            .product-item-wrapper {
              .product-item-bottom {
                @include transform(translateY(0));
              }
            }
          }
        }
      }
    }
  }
}
.pagination {
  padding-left: $row_padding;
  padding-right: $row_padding;
  .icon {
    padding: 3px;
  }
  li {
    color: $color_text_general;
    &.pagination__text {
      background: #000;
      color: #fff;
      padding: 0 $row_padding;
      span {
        color: #fff;
        background: none;
        border-color: #000;
      }
    }
    a.btn--secondary,
    .btn--secondary {
      padding: 0;
      height: 32px;
      line-height: 32px;
      width: 32px;
      text-align: center;
      border: 2px solid #000;
      background: #fff;
      @include opacity(1,100);
      @include border_radius(0);
      @include transition($transition-base);
      &:before {
        display: none;
      }
      &:hover {
        background: #000;
        border-color: #000;
        .icon-cdz {
          color: #fff;
        }
      }
      .icon-cdz {
        color: #000;
        padding: 0;
        margin: 0 auto;
        height: 30px;
      }
    }
    >div {
      padding: 0;
    }
    a {
      font-weight: normal;
      padding: 0 $row_padding;
      background: $bg_paging;
      border: 1px solid $border_general;
      color: $color_text_heading;
      font-size: 15px;
      margin: 0;
      height: 40px;
      line-height: 40px;
      border-left: 0;
      position: relative;
      @include border_radius(0);
      &:hover {
        text-decoration: none; 
        background-color: $bkg_button_hover; 
        color: $color_text_button_hover; 
        border-color: $color_border_button_hover;
      }
    }
    &.active {
      a, a:hover {
        text-decoration: none; 
        background-color: $bkg_button_hover; 
        color: $color_text_button_hover; 
        border-color: $color_border_button_hover;
      }
    }
    &.prev,
    &.next {
      a{
        width: 85px;
        text-align: left;
        @include border_radius(0);
        &:before {
          content: 'PREV';
          position: absolute;
          right: 0;
          top: 0;
          margin-right: 15px;
        }
      }
    }
    &.next {
      a{
        text-align: right;
        &:before {
          content: 'NEXT';
          right: auto;
          left: 0;
          margin-right: 0;
          margin-left: 15px;
        }
      }
    }
  }
}
