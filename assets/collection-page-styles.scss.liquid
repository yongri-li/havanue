/*Collections*/
.list-collection-page {
  .col-main-containter {
    > .row {
      >div {
        padding-left: 0;
        padding-right: 0;
      }
    }
  }
}
.collection-grid-item {
  overflow: hidden;
  .collection-grid-item__link {
    @include effect_product_main();
    .collection-grid-item__title {
      @include transition($transition-base);
      @include transform(rotateY(-30px));
      @include opacity(0,0);
      @media(max-width: 1024px) {
        @include transform(rotateY(0));
        @include opacity(1,100);
      }
    }
    &:hover {
      @include effect_product_hover();
      .collection-grid-item__title {
        @include transform(rotateY(0));
        @include opacity(1,100);
      }
    }
  }
}
body.template-collection {
  @media(max-width: 991px) {
    &.cdz-off-canvas-wrap {
      position: relative;
      @include inline-block();
      width: 100%;
      .col-main-containter {
        .row{
          >div{
            position: static;
          }
        }
      }
      .wrapper{
        @include transition($transition-base);
        position: static;
        .main-container{
          position: static;
        }
      }
      .main-sidebar {
        &.sidebar .block:not(.block-static-block) {
          @media(max-width: 991px) {
            background: #fff;
            padding: 20px;
          }
        }
        .navbar-canvas-inner {
          width: $width_navbar;
          @media(max-width: 991px) {
            background: #fff;
            .canvas-content {
              background: $bkg_nav_header_mobile;
            }
          }
          .cdz-menu-logo {
            padding: 6px 20px;
            border-bottom: 1px solid $border_menu_mobile;
            background: $bkg_main_header;
            .collection-title {
              color: $color_text_menu;
            }
          }
        }
        &.hide-sidebar {
          @media(min-width: 992px) {
            position: absolute;
            height: auto;
            width: 250px !important;
            display: none;
            .block {
              display: block;
              @include opacity(1,100);
              @include transform(scale(1,1));
            }
          }
        }
      }
      &.move-left{
        .wrapper{
          @include transform(translate3d(-250px,0,0));
          #cdz-exit-off-canvas{
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            width: 100%;
            height: 100%;
            background: rgba(0,0,0,0.2);
          }
        }
        .main-sidebar {
          @include transform(translate3d(0,0,0));
          overflow-y: scroll;
          display: block;
          &.hide-sidebar {
            @media(min-width: 991px) {
              display: block;
            }
          }
        }
      }
    }
  }
}
