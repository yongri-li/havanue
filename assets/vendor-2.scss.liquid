/*Instagram block*/
.instagram-slider-section {
  .instaram-wrapper {
    .block-title {
      text-transform: initial;
      margin-bottom: $margin_distance;
    }
    &:hover {
      .owl-nav {
        @include opacity(1,100);
      }
    }
    .owl-nav{
      @media(max-width: 767px) {
        display: none;
      }
      >div {
        margin-top: -60px;
        &.owl-prev {
          margin-left: $row_padding/2;
        }
        &.owl-next {
          margin-right: $row_padding/2;
        }
      }
    }
    .instagram-slider {
      margin-left: -$row_padding;
      margin-right: -$row_padding;
      margin-top: $margin_distance;
      @media(max-width: 767px) {
        margin-top: $margin_distance_mobile;
      }
      .use-slider-custom {
        margin-left: 5px;
        margin-right: 5px;
      }
      .image-list {
        .owl-stage-outer {
          @media(max-width: 767px) {
            margin-left: -$row_padding/2;
            margin-right: -$row_padding/2;
          }
        }
        .item {
          .instagram-media {
            padding: 0 $row_padding/2;
            position: relative;
            &:hover {
              .image {
                &:before {
                  @include opacity(1,100);
                  @media(max-width: 767px) {
                    @include opacity(0,0);
                    display: none;
                  }
                }
              }
              .action {
                @include opacity(1,100);
                @include transform(translateY(0));
                @media(max-width: 767px) {
                  @include opacity(0,0);
                  display: none;
                }
              }
            }
            .image {
              display: block;
              position: relative;
              &:before {
                content: '';
                position: absolute;
                top: 0;
                left: 0;
                right: 0;
                bottom: 0;
                width: 100%;
                height: 100%;
                background: rgba(0,0,0,0.4);
                @include transition($transition-base);
                @include opacity(0,0);
              }
            }
            .action {
              position: absolute;
