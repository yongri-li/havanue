@mixin prefix($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } 
    @else if $prefix == moz {
      -moz-#{$property}: $value;
    } 
    @else if $prefix == ms {
      -ms-#{$property}: $value;
    } 
    @else if $prefix == o {
      -o-#{$property}: $value;
    } 
    @else if $prefix == spec {
      #{$property}: $value;
    } 
    @else  {
      @warn 'Unrecognized prefix: #{$prefix}';
    }
  }
}
@mixin display-flexbox() {
  display: -webkit-flex;
  display: -ms-flex;
  display: flex;
  flex-flow: row wrap;
}
@mixin align-items($value: stretch) {
  $alt-value: $value;

  @if $value == 'flex-start' {
    $alt-value: start;
  } @else if $value == 'flex-end' {
    $alt-value: end;
  }

  /* sass-lint:disable no-misspelled-properties*/
  -ms-flex-align: $alt-value;
  @include prefix(align-items, $value, webkit moz ms o spec);
}
@mixin flex-direction($value) {
  @include prefix(flex-direction, $value, webkit moz ms webkit);
}
@mixin overflow-scrolling($var) {
  @include prefix(overflow-scrolling, $var, webkit moz ms webkit);
}
@mixin slider-preload($minwidth) {
  @media(min-width: 1025px) {
    @include display-flexbox();
  }
  overflow: hidden;
  .item {
    min-width: $minwidth;
  }
}

$row_padding: 10px;

@mixin slider-horizontal() {
  overflow-x: auto;
  overflow-y: hidden;
  @include display-flexbox();
  flex-flow: row nowrap!important;
  white-space: nowrap;
  @include overflow-scrolling(touch);
  &::-webkit-scrollbar { 
      display: none;
  }
  .item {
    @include display-flexbox();
    .product-item-wrapper {
      width: 100%;
      height: auto !important;
      @include display-flexbox();
      @include flex-direction(column);
      @include align-items(center);
      .product-details {
        white-space: normal;
      }
    }
  }
}

.use-slider-custom {
  @media(max-width: 1024px) {
    @include slider-horizontal();     
  }
  &:not(.owl-loaded) {
