/*------------Custom content-------------*/
.custom-content{
  @include display-flexbox();
  @include justify-content(center);
  >div{
    padding: 0 $row_padding;
  }
}
/*-------------End Custom content-------------*/

/*Modal wishlist*/
$width_button_wishlist: 30px;
#wishlistModal {
  table tbody td {
    .product-name {
      a {
        font-weight: $font_weight_normal;
        text-transform: capitalize;
      }
    }
    .qty_cart {
      @include display-flexbox();
      input.qty {
        width: $width_button_wishlist !important;
        min-width: $width_button_wishlist;
        height: $width_button_wishlist !important;
      }
    }
    .qty-ctl button {
      height: $width_button_wishlist;
      line-height: $width_button_wishlist;
      width: $width_button_wishlist;
    }
    .qty-ctl button.increase:before {
      line-height: 1.2;
    }
    .add-cart-wishlist,
    .remove-wishlist {
      @include inline-block();
      padding: 0;
      text-align: center;
      width: $width_button_wishlist;
      height: $width_button_wishlist;
      line-height: $width_button_wishlist;
      border: 1px solid $color_border_button;
      color: $color_text_button;
      background: $bkg_button;
      @include transition($transition-base);
      vertical-align: bottom;
      &:after, 
      &:before {
        line-height: $width_button_wishlist;
      }
      .icon-cdz {
        color: $color_text_button;
        font-size: 15px;
      }
      &:hover {
        border-color: $color_border_button_hover;
        color: $color_text_button_hover;
        background: $bkg_button_hover;
        .icon-cdz {
          color: $color_text_button_hover;
        }
      }
    }
  }
  .modal-body {
    @media(max-width: 1024px) {
      overflow-y: scroll;
    }
  }
  .form_cart table tr {
    .shoppingcart-name,
    .shoppingcart-tax,
    .shoppingcart-qty,
    .product-options {
      @media(max-width: 767px) {
        padding-right: 5px;
        padding-left: 5px;
        padding-top: 10px;
        width: 100px;
      }
    }
    .product-options {
      width: 100px;
      @media(max-width: 599px) {
        width: 100%;
      }  
    }
    @media (min-width: 600px) {
      .product-image {
        width: 100px;
        img {
          width: 100%;
        }
      }
    }
    @media (max-width: 599px) {
      .shoppingcart-name {
        width: 100%;
      }
    }
  }
}
/*End Modal wishlist*/
