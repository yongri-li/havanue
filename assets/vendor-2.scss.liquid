              font-size: 12px;
              font-family: $font_general;
              text-transform: uppercase;
              display: block;
              text-align: center;
              height: $height_button;
              line-height: $height_button - 2px;
              color: $color_text_button2;
              border: 1px solid $color_border_button2;
              background: $bkg_button2;
              @include transition($transition-base);
              &.link-compare {
                background: rgba($bkg_button2,0.95);
              }
              .icon-cdz {
                display: none;
                color: $color_text_button2;
                @media(max-width: 1024px) {
                  display: block;
                  margin: 0 auto;
                  height: $height_button;
                  line-height: $height_button - 2px;
