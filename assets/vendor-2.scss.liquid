.use-slider {
  &:not(.owl-loaded) {
    @include slider-preload(50%);
  }
  .product-item .product-item-wrapper .product-item-bottom {
    position: static !important;
  }
  .product-item .product-item-wrapper {
    position: relative;
  }
  .more-views-slider {
    right: 0;
    left: auto;
  }
  &:hover {
    .owl-nav {
      @include opacity(1,100);
    }
  }
}
.use-slider, .use-slider-custom {
  margin-left: -$row_padding;
  margin-right: -$row_padding;  
}
.use-switch-slider-grid {
  @media(max-width: 767px) {
    margin-left: -$row_padding;
    margin-right: -$row_padding;
  }
}
.more-views-slider {
  .image-list {
    width: 44px;
    .item {
      width: 100%;
      height: 56px;
      border: 1px solid $input_border;
    }
    &.lSSlide {
      img {
        height: 100%;
        width: 100%;
      }
    }
  }
  .lSSlideOuter {
    &.vertical {
      padding-top: 20px;
      overflow: initial;
    }
  }
  .lSAction {
    > a {
      width: 100%;
      height: 10px;
      color: $color_text_heading;
      font-size: 20px;
      background: none;
      text-align: center;
      left: 0;
      right: 0;
      margin: 0 auto;
      &:before {
        content: '\f106';
        font-family: FontAwesome;
      }
      &.lSPrev {
        top: 0;
        margin-top: -10px;
        left: 0;
        right: 0;
        margin-left: auto;
        margin-right: auto;
      }
      &.lSNext {
        bottom: 0;
        margin-bottom: -30px;
        @include transform(rotateZ(-180deg));
        left: 0;
        right: 0;
        margin-left: auto;
        margin-right: auto;
      }
    }
  }
}
