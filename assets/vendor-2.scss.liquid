@mixin opacity_important($var1, $var2) {
  opacity: $var1 !important;
  filter: alpha(opacity=$var2) !important;
  -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=$var2)" !important;
}
.slick-prev,
.slick-next {
  z-index: 998;
  width: 32px;
  height: 32px;
  &:before{
    content: '';
    width: 32px;
    display: block;
    top: 50%;
    height: 32px;
    background-image: url({{ 'ls_controls.png' | asset_url }});
    cursor: pointer;
    position: absolute;
    margin-top: -16px;
    @include opacity_important(0,0);
  }
  &.slick-disabled {
    &:hover {
      &:before {
        cursor: text;
      }
    }
  }
}
.slick-prev {
  left: 0;
  &:before {
    background-position: 0 0;
  }
}
.slick-next {
  right: 0;
  &:before {
    background-position: -32px 0;
  }
}
.slick-slider {
  &:hover {
    .slick-prev,
    .slick-next {
      &:before {
        @include opacity_important(1,100);
      }
    }
  }
  .slick-prev,
  .slick-next {
    &:before {
      @media(max-width: 991px) {
        @include opacity_important(1,100);
      }
    }
  }
  &.slick-vertical {
    .slick-slide {
      margin-bottom: 5px;
    }
  }
  &:not(.slick-vertical) {
    .slick-slide {
      margin-right: 5px;
    } 
  }
}
.viewmore-main {
  &.slick-slider {
    .slick-slide {
      margin-right: 0;
    }
  }
}
.slick-vertical {
  @media(min-width: 992px) {
    .slick-prev {
      top: 0;
      left: 0;
      right: 0;
      margin: 0 auto;
      text-align: center;
      &:before {
        background-position: 0 32px;
      }
    }
    .slick-next {
      top: 100%;
      left: 0;
      right: 0;
      margin: 0 auto;
      text-align: center;
      &:before {
        background-position: -32px 32px;
      }
    }
  }
}

