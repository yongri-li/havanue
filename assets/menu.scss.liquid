/*column menu*/
$column_width_s:710px;
$column_width_m:930px;
$column_width_l:1140px;

@mixin generate-columns($n, $col_width){
	@for $i from 1 through $n {
		.cdz-menu .groupmenu-width-#{$i} {
			width: ($col_width / 24 * $i);
		}
		.cdz-menu .groupmenu-width-#{$i}.row {
