              @media(min-width: 1025px) {
                &:hover {
                  color: $color_text_button2_hover;
                  border: 1px solid $color_border_button2_hover;
                  background: $bkg_button2_hover;
                  .icon-cdz {
                    color: $color_text_button2_hover;
                  }
                }
              }
              .icon-cdz {
                color: $color_text_button2;
              }
              span:not(.icon-cdz) {
                display: none;
              }
            }
          }
          .link-compare {
            width: 50%;
            float: left;
            text-align: center;
            height: $height_button;
            line-height: $height_button;
            padding: 0 15px;
            color: $color_text_button2;
            border: 1px solid $color_border_button2;
            background: $bkg_button2;
            @media(min-width: 1025px) {
              &:hover {
                color: $color_text_button2_hover;
                border: 1px solid $color_border_button2_hover;
                background: $bkg_button2_hover;
                .icon-cdz {
                  color: $color_text_button2_hover;
                }
              }
            }
          }
        }
      }
    }
  }
  .product-details {
    .product-price__price {
      margin-top: 5px;
      font-weight: $font_weight_bold;
      font-size: 18px;
    }
    .product-name {
      a {
        font-size: 16px;
      }
    }
  }
  .block-content {
    .part-block {
      .col-block {
        padding: 33px 60px;
        @media(max-width: 767px) {
          width: 100%;
          padding: $row_padding;
        }
        .part-title {
          font-size: 24px;
          text-transform: capitalize;
          color: $color_text_heading;
        }
        .part-desc {
          margin-top: 5px;
          margin-bottom: 25px;
        }
        .product-list {
          .product-item {
            .product-details {
              text-align: center;
              .spr-badge {
                text-align: center;
                overflow: hidden;
                .spr-badge-starrating {
                  position: relative;
                  @include inline-block();
                  padding: 0 $row_padding;
                  margin: 0;
                  @media(max-width: 767px) {
                    padding: 0;
                  }
                  &:after, 
                  &:before {
                    content: '';
                    position: absolute;
                    bottom: 53%;
                    width: 1000px;
                    height: 1px;
                    background: $border_general;
                  }
                  &:before {
                    left: -1010px;
                  }
                  &:after {
                    right: -1010px;
                  }
                }
              }
            }
          }
        }
      }
      .col-banner {
        @media(max-width: 767px) {
          width: 100%;
          margin-top: $margin_distance;
        }
        .banner {
          img {
            width: 100%;
          }
        }
      }
      &.part-block-1 {
        .col-banner {
          padding-left: 0;
          @media(max-width: 767px) {
            padding-left: $row_padding;
          }
        }
      }
      &.part-block-2 {
        .col-block {
          float: right;
        }
        .col-banner {
          padding-right: 0;
          @media(max-width: 767px) {
            padding-right: $row_padding;
          }
        }
      }
    }
  }
  .owl-dots {
    @include display-flexbox();
    @include justify-content(center);
    @media(max-width: 767px) {
      margin-top: $row_padding;
    }
    >div {
      padding: 0 5px;
      span {
        @include inline-block();
        width: 10px;
        height: 10px;
        background: #ccc;
        @include border_radius(50%);
      }
      &.active {
        span {
          background: $color_hover_general;
        }
      }
    }
  }
  .use-slider-custom .item {
    @media(max-width: 1024px) {
      min-width: 45%;
    }
    @media (max-width: 1024px) and (max-width: 991px) {
      min-width: 60%;
    }
    @media(max-width: 767px) {
      min-width: 45%;
    }
  }
}