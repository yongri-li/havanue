.global-hero {
  position: relative;
  &.full_width {
    max-width: 100%;
  }
  &.fixed_width {
    max-width: $container;
  }
  &.adapt {
    height: 100%;
  }
  &.x-small {
    height: $xs-height;
    @media (max-width: 767px) {
      height: $xs-mob-height;
    }
  }
  &.small {
    height: $sm-height;
    @media (max-width: 767px) {
      height: $sm-mob-height;
    }
  }
  &.medium {
    height: $medium-height;
    @media (max-width: 767px) {
      height: $medium-mob-height;
    }
  }
  &.large {
    height: $lg-height;
    @media (max-width: 767px) {
      height: 573px;
    }
  }
  &.x-large {
    height: $x-lg-mob-height;
  }
  .global-hero__image-wrapper {
  	height: 100%;
    img {
    	height: 100%;
      	width: 100%;
      	object-fit: cover;
    }
  }
  .global-hero__body {
    position: absolute;
    width: 90%;
    padding-left: 40px;
    @media (max-width: 767px) {
    	padding-left: 0px;
    }
    &.left {
    	text-align: left;
      * {
      	text-align: left;
      }
    }
     &.center {
    	text-align: center;
      * {
      	text-align: center;
      }
    }
     &.right {
    	text-align: right;
      * {
      	text-align: right;
      }
    }
    &.top.left {
      top: $margin;
      left: $margin;
      @media (max-width: 767px) {
        top: $mob-margin;
        left: $mob-margin;
      }
    }
    &.top.center {
      top: $margin;
      left: 50%;
      transform: translateX(-50%);
      @media (max-width: 767px) {
        top: $mob-margin;
      }
    }
    &.top.right {
      top: $margin;
      right: $margin;
      @media (max-width: 767px) {
        top: $mob-margin;
        right: $mob-margin;
      }
    }
    &.middle.left {
      top: 50%;
      transform: translateY(-50%);
      left: $margin;
      @media (max-width: 767px) {
        left: $mob-margin;
      }
    }
    &.middle.center {
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }
    &.middle.right {
      top: 50%;
      transform: translateY(-50%);
      right: $margin;
      @media (max-width: 767px) {
        right: $mob-margin;
      }
    }
    &.bottom.left {
      bottom: $margin;
      left: $margin;
      @media (max-width: 767px) {
        left: $mob-margin;
      }
    }
    &.bottom.center {
      bottom: $margin;
      left: 50%;
      transform: translateX(-50%);
      @media (max-width: 767px) {
        bottom: $mob-margin;
      }
    }
    &.bottom.right {
      bottom: $margin;
      right: $margin;
      @media (max-width: 767px) {
        bottom: $mob-margin;
        right: $mob-margin;
      }
    }
    .global-hero__subtitle {
      font-family: "Montserrat", sans-serif;
      font-size: 23px;
      font-style: normal;
      font-weight: 400;
      line-height: 1.55;
      letter-spacing: 0em;
      color: #282828;
      @media (max-width: 767px) {
        font-size: 14px;
      }
    }
    .global-hero__title {
		font-family: "Montserrat", sans-serif;
        font-size: 54px;
        font-style: normal;
        font-weight: 400;
        line-height: 1.55;
        letter-spacing: 0em;
		color: #282828;
        text-transform: capitalize;
      	margin-bottom: 0;
      	margin-top: 25px;
      @media (max-width: 1023px) and (min-width: 768px) {
      	font-size: 37px;
      }
      	@media (max-width: 767px) {
          font-size: 26px;
          margin-top: 14px;
          margin-bottom: 12px;
        }
    }
    .global-hero__text p {
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 400;
      line-height: 1.65;
      letter-spacing: 0em;
      color: #000;
      margin-top: 7px;
      margin-bottom: 50px;
      @media (max-width: 767px) {
        font-size: 15px;
        margin-top: 14px;
        margin-bottom: 12px;
        margin-bottom: 26px;
      }
    }
    .btn.btn-primary {
    	padding-left: 51px;
      	padding-right: 51px;
      @media (max-width: 767px) {
      	font-size: 21px;
      }
    }
  }
}



